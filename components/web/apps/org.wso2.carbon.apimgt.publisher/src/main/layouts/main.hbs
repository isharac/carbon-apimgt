<!DOCTYPE html>
<html lang="en">
<head>
    <title>{{#placeholder "title"}}API Publisher{{/placeholder}}</title>
    <meta charset="UTF-8">
    <meta name="description" content=""/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <!-- Le fav and touch icons -->
    {{#placeholder "favicon"}}
        <link rel="shortcut icon" href="{{public "images/default-favicon.png"}}" type="image/png" />
    {{/placeholder}}
    {{placeholder "css"}}
    {{placeholder "headJs"}}
    {{placeholder "headOther"}}
</head>
<body class="sticky-footer ">
<!-- header -->
{{defineZone "topbar"}}

<!-- End header -->
<!--Breadcumb -->
<div class="breadcrumb-wrapper">
    <ol class="breadcrumb">
        <li>
            <a href="/publisher/site/pages/index.jag" title="Home" onclick="jagg.sessionAwareJS({redirect:'/publisher/site/pages/index.jag',e:event})"><i class="icon fw fw-home"></i> <span class="hidden-xs">Home</span></a>
        </li>
        <li class="active">
            <a href="/publisher/apis" title="API List" onclick="jagg.sessionAwareJS({redirect:'/publisher/apis',e:event})">APIs</a>
        </li>
    </ol>
</div>

<!--End Breadcumb -->
<!-- #page-content-wrapper -->
<div class="page-content-wrapper" style="padding-left: 300px;">
    <div class="navbar-wrapper">
        <nav class="navbar navbar-default" data-spy="affix"
             data-offset-top="80" data-offset-bottom="40">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed"
                            data-toggle="collapse" data-target="#navbar" aria-expanded="false"
                            aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span> <span
                            class="icon-bar"></span> <span class="icon-bar"></span> <span
                            class="icon-bar"></span>
                    </button>
                    <a class="navbar-menu-toggle collapsed hidden"
                       data-toggle="collapse" data-target="#navbar2" aria-expanded="false"
                       aria-controls="navbar2" title="navigator bar"> <span class="icon fw-stack"> <i
                            class="fw fw-tiles fw-stack-1x toggle-icon-up"></i>
					</span>

                    </a>

                    <a class="navbar-menu-toggle active" data-toggle="sidebar" data-target="#left-sidebar" data-container=".page-content-wrapper" data-container-divide="true" aria-expanded="true" rel="leftmenu-sidebar">
                        <span class="icon fw-stack">
                            <i class="fw fw-menu fw-stack-1x toggle-icon-cancel"></i>
                        </span>
                    </a>
                </div>

                <!-- Actions Menu -->
                {{defineZone "actionMenu"}}
                <!-- End Actions Menu -->
            </div>
        </nav>
    </div>

    {{defineZone "secondaryMenu"}}


    <!-- page content -->
    <div class="container-fluid">
        <div class="body-wrapper" id="bodyWrapper">
            {{defineZone "content"}}
        </div>
    </div>
</div>
<!-- /#wrapper -->
<footer class="footer">
    <div class="container-fluid">
        {{defineZone "footer"}}    </div>
</footer>
{{placeholder "js"}}
</body>
</html>